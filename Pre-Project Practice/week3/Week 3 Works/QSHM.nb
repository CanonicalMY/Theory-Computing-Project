Cell[CellGroupData[{

Cell["\<\
Use the function above to check this is indeed an eigenfunction H \
\[CapitalPsi]n = En \[CapitalPsi]n.
Check the energy matches expectation. If you have a recent version of \
mathematica (\[GreaterEqual]12.3), the SolveValues function is nice, but \
otherwise the Solve function works fine.\
\>", "Subsection",
 CellChangeTimes->{{3.874053007029961*^9, 3.8740530528773923`*^9}, 
   3.87690122166634*^9, {3.885135029333764*^9, 3.885135031143775*^9}, 
   3.885140617988352*^9, {3.8851460876104*^9, 
   3.885146162117651*^9}},ExpressionUUID->"280b5091-6f0a-487b-81b2-\
f6e751868c02"],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"H", "[", "psi_", "]"}], ":=", 
   RowBox[{
    RowBox[{"1", "/", "2"}], "*", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", 
       RowBox[{"D", "[", 
        RowBox[{"psi", ",", 
         RowBox[{"{", 
          RowBox[{"x", ",", "2"}], "}"}]}], "]"}]}], "+", 
      RowBox[{
       RowBox[{"x", "^", "2"}], " ", "psi"}]}], ")"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"eqn", "=", 
   RowBox[{
    RowBox[{"H", "[", 
     RowBox[{"Psi", "[", "n", "]"}], "]"}], "==", 
    RowBox[{"En", " ", 
     RowBox[{"Psi", "[", "n", "]"}]}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"EneigenValue", "=", 
   RowBox[{"SolveValues", "[", 
    RowBox[{"eqn", ",", "En"}], "]"}]}], ";"}], "\n", 
